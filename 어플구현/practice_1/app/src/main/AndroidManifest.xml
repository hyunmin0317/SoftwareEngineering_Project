<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practice_1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Practice_1">
        <!--로그인화면을 맨 처음 화면으로 만들어 준 것. 다른 화면을 원한다면 intent-filter를 다른 화면 아래에 넣어주세요.-->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Mileage"></activity>
        <activity android:name=".coupon" />
        <activity android:name=".PersonInfo" />
        <activity android:name=".MainhomeActivity" />
        <activity android:name=".Reserve_1" />
        <activity android:name=".Signup" />
        <activity android:name=".payment_1" />

    </application>

</manifest>